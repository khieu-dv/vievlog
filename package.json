{
  "name": "relivator",
  "version": "1.4.1",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "next build",
    "build:rust": "curl --proto '=https' --tlsv1.2 -sSf https://sh.rustup.rs | sh -s -- -y && . \"$HOME/.cargo/env\" && cd rust-wasm && wasm-pack build --target bundler --out-dir ../src/wasm",
    "postbuild": "node script-search/build-search.cjs",
    "dev": "next dev --turbopack",
    "db:push": "drizzle-kit push",
    "db:auth": "bun addons/better.ts",
    "db:studio": "drizzle-kit studio",
    "ui": "bun x --bun shadcn@canary add",
    "latest": "bun update --latest && bun check",
    "check": "knip && tsc --noEmit && eslint . && biome check --write .",
    "generate-meta": "node scripts/generate-meta.js",
    "generate-posts": "node scripts/generate-posts.js",
    "generate-posts-demo": "node scripts/generate-posts-demo.js",
    "generate-all": "npm run generate-meta && npm run generate-posts",
    "generate-demo": "npm run generate-meta && npm run generate-posts-demo"
  },
